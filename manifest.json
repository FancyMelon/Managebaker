{
  "author": "Felix Liu, Martin Wang",
  "icons": {
    "16": "lib/icon/icon16.png",
    "48": "lib/icon/icon48.png",
    "128": "lib/icon/icon128.png"
  },
  "browser_action": {
    "default_title": "Managebaker",      
    "default_popup": "managebaker/popup.html"      
  },
  "content_scripts": [{
    "matches": [
      "https://*.managebac.cn/*",
      "https://*.managebac.com/*"
    ],
    "exclude_matches": [
      "https://www.managebac.cn/*",
      "https://www.managebac.com/*",
      "https://subjectcentre.managebac.cn/*",
      "https://subjectcentre.managebac.com/*"
    ],
    "js": [
      "lib/jquery-3.3.1.js",
      "managebaker/managebaker.js",
      "managebaker/assignment.js",
      "lib/Chart_min.js",
      "lib/material/material.js",
      "lib/usefulUtil.js"
    ],
    "css": [
      "managebaker/stylesheet.css",
      "lib/material/material.css"
      
    ],
    "run_at": "document_idle"
  }],
  "background": {
    "scripts": [
      "background/background.js",
      "lib/jquery-3.3.1.js",
      "lib/localforage.min.js",
      "lib/usefulUtil.js",
      "background/event.js"
    ],
    "persistent": false
  },
  "options_page": "managebaker/options.html",
  "description": "自动算分，告别脱发！\nCheck your scores, succeed in IB!",
  "manifest_version": 2,
  "name": "Managebaker",
  "permissions": [
    "https://*.managebac.cn/*",
    "https://*.managebac.com/*",
    "tabs"
  ],
  "version": "3.2.2"
}