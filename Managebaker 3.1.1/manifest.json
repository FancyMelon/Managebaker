{
  "author": "Felix Liu",

  "icons": { "16": "icon16.png", "48": "icon48.png", "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": [
        "https://*.managebac.cn/student/classes/*/assignments",
        "https://*.managebac.com/student/classes/*/assignments"
      ],
      "js": ["jquery-3.3.1.js", "managebaker3.1.1.js", "Chart_min.js"],
      "css": ["replace.css"]
    }
  ],
  "description": "自动算分，告别脱发！\nCheck your scores, succeed in IB!",
  "manifest_version": 2,
  "name": "Managebaker",
  "permissions": [
    "activeTab",
    "https://*.managebac.cn/*",
    "https://*.managebac.com/*"
  ],
  "version": "3.1.1"
}
